<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="180" prefWidth="500"
    fx:controller="com.example.ControllerExample7">
    <children>
        <Label layoutX="11" layoutY="9" text="ListView"/>
        <ListView fx:id="listView" layoutX="10" layoutY="30" prefHeight="100" prefWidth="100"/>

        <Label layoutX="125" layoutY="9" text="TableView"/>
        <TableView fx:id="tableView" layoutX="120" layoutY="30" prefHeight="100" prefWidth="290">
            <columns>
                <TableColumn prefWidth="100" text="SmartPhone"/>
                <TableColumn prefWidth="100" text="Image"/>
            </columns>
        </TableView> 

        <Label layoutX="425" layoutY="9" text="ImageView"/>
        <ImageView fx:id="imageView" fitHeight="100" fitWidth="60" layoutX="430" layoutY="30" preserveRatio="true"/>

        <Button layoutX="190" layoutY="145" onAction="#handleBtnOkAction" text="OK"/>
        <Button layoutX="260" layoutY="145" onAction="#handleBtnCancelAction" text="Cancel"/>
    </children>
</AnchorPane>